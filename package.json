  "license": "MIT",
  "bugs": {
    "url": "https://github.com/allting/CycleStarter/issues"
  },
  "homepage": "https://github.com/allting/CycleStarter#readme",
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.6.5",
    "babel-core": "6.7.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-register": "6.7.2",
    "chai": "*",
    "mocha": "*",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
